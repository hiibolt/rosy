BEGIN;
{Test the extract operator with strings, vectors, and complex numbers}

VARIABLE (ST) test_string;
VARIABLE (VE) test_vector;
VARIABLE (CM) test_complex;

{Test string extraction}
test_string := "Hello";
WRITE 6 "String: " test_string;
WRITE 6 "Character 1: " ST(test_string|1) ;
WRITE 6 "Character 3: " ST(test_string|3) ;

{Test vector extraction}
test_vector := 10 & 20 & 30;
WRITE 6 "Vector: " ST(test_vector);
WRITE 6 "Element 1: " ST(test_vector|1) ;
WRITE 6 "Element 2: " ST(test_vector|2) ;

{Test complex extraction}
test_complex := CM(5 & 7);
WRITE 6 "Complex: " ST(test_complex);
WRITE 6 "Real part: " ST(test_complex|1);
WRITE 6 "Imaginary part: " ST(test_complex|2);

END;